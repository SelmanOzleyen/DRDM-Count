{
    "jupyter.jupyterServerType": "local",
    "python.pythonPath": "/bin/python3",
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/.hg/store/**": true,
        "**/conf_logs/**": true,
        "**/runs/**": true,
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/venv/**": true,
        "**/.git/**": true,
        "**/conf_logs/**": true,
        "**/runs/**": true,
        "**/ckpts/**": true,
        },
        "spellright.language": [
            "ru_RU"
        ],
        "spellright.documentTypes": [
            "latex"
        ],
}